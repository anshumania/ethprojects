<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 EJB 3.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_1-0.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>CompanySession</ejb-name>
      <resource-ref>
        <res-ref-name>jms/CompanyTasksFactory</res-ref-name>
        <jndi-name>jms/CompanyTasksFactory</jndi-name>
      </resource-ref>
      <resource-ref>
        <res-ref-name>jms/ControlOfficeFactory</res-ref-name>
        <jndi-name>jms/ControlOfficeFactory</jndi-name>
      </resource-ref>
      <message-destination-ref>
        <message-destination-ref-name>jms/CompanyTasks</message-destination-ref-name>
        <jndi-name>jms/CompanyTasks</jndi-name>
      </message-destination-ref>
      <message-destination-ref>
        <message-destination-ref-name>jms/ControlOffice</message-destination-ref-name>
        <jndi-name>jms/ControlOffice</jndi-name>
      </message-destination-ref>
    </ejb>
    <ejb>
      <ejb-name>LocalOfficeTwoz</ejb-name>
    </ejb>
    <ejb>
      <ejb-name>LocalOfficeOnez</ejb-name>
    </ejb>
    <message-destination>
      <message-destination-name>jms/CompanyTasks</message-destination-name>
      <jndi-name>jms/CompanyTasks</jndi-name>
    </message-destination>
  </enterprise-beans>
</sun-ejb-jar>
