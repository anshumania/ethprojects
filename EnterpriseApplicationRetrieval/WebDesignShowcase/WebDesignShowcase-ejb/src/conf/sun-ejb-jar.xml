<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 EJB 3.1//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_1-0.dtd">
<sun-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>CommentSessionBean</ejb-name>
      <resource-ref>
        <res-ref-name>CommentsFactory</res-ref-name>
        <jndi-name>CommentsFactory</jndi-name>
      </resource-ref>
      <resource-ref>
        <res-ref-name>TopicConnectionFactory</res-ref-name>
        <jndi-name>TopicConnectionFactory</jndi-name>
      </resource-ref>
      <message-destination-ref>
        <message-destination-ref-name>Comments</message-destination-ref-name>
        <jndi-name>Comments</jndi-name>
      </message-destination-ref>
    </ejb>
  </enterprise-beans>
</sun-ejb-jar>
